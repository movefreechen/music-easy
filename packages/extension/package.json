{
  "name": "music-easy",
  "displayName": "music easy",
  "description": "表格风音乐播放器",
  "publisher": "movefreechen",
  "icon": "./logo.png",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "music.easy.start",
        "title": "Music Easy"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "music easy configuration",
      "properties": {
        "music.easy.proxy": {
          "type": "string",
          "default": "",
          "description": "http/https代理地址，例如 http://username:password@127.0.0.1:8888"
        },
        "music.easy.port": {
          "type": "number",
          "default": 4000,
          "description": "音乐api服务端口，默认4000"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "buildApi": "ncc build node_modules/NeteaseCloudMusicApi/app.js -o public -a"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/kill-port": "^2.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vercel/ncc": "^0.36.1",
    "@vscode/test-electron": "^2.3.2",
    "babel-loader": "^9.1.2",
    "babel-plugin-static-fs": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "esbuild": "^0.18.11",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "html-webpack-plugin": "^5.5.3",
    "mocha": "^10.2.0",
    "pkg": "^5.8.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "NeteaseCloudMusicApi": "^4.9.2",
    "axios": "^1.4.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "handlebars": "^4.7.7",
    "kill-port": "^2.0.1",
    "mri": "^1.2.0",
    "pac-proxy-agent": "^6.0.3",
    "tunnel": "^0.0.6",
    "vm2": "^3.9.19"
  },
  "pkg": {
    "scripts": "node_modules/NeteaseCloudMusicApi/module/*.js",
    "targets": [
      "node14-linux-x64",
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "assets": [
      "node_modules/axios",
      "node_modules/express",
      "node_modules/express-fileupload",
      "node_modules/md5",
      "node_modules/music-metadata",
      "node_modules/pac-proxy-agent",
      "node_modules/qrcode",
      "node_modules/safe-decode-uri-component",
      "node_modules/tunnel",
      "node_modules/yargs",
      "node_modules/tslib",
      "node_modules/vm2",
      "module",
      "public"
    ],
    "outputPath": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movefreechen/music-easy"
  }
}