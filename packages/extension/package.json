{
	"name": "music-easy-extension",
	"displayName": "music-easy-extension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.79.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"browser": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "catCoding.start",
				"title": "Start new cat coding session",
				"category": "Cat Coding"
			}
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/test/suite/index.js",
		"pretest": "pnpm run compile-web",
		"vscode:prepublish": "pnpm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/mocha": "^10.0.1",
		"@types/node": "^20.3.3",
		"@types/vscode": "^1.79.0",
		"@types/webpack-env": "^1.18.1",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@vscode/test-web": "^0.0.44",
		"assert": "^2.0.0",
		"eslint": "^8.41.0",
		"html-webpack-plugin": "^5.5.3",
		"mocha": "^10.2.0",
		"path-browserify": "^1.0.1",
		"process": "^0.11.10",
		"ts-loader": "^9.4.3",
		"typescript": "^5.1.3",
		"webpack": "^5.85.0",
		"webpack-cli": "^5.1.1"
	},
	"dependencies": {
		"handlebars": "^4.7.7"
	}
}